var mt = mt || {
    track: function(event, data){
        mt.rq.push({action: 'track', event: event, data: data, time: new Date()});
    },
    identify: function(data){
        mt.rq.push({action: 'identify', data:data});
    },
    clear: function(){
        mt.rq.push({action: 'clear'});
    }
};

mt.rq = mt.rq || [];